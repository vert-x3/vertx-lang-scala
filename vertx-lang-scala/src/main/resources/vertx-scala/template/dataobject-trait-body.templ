@foreach{property:properties}
@if{property.doc != null}
\n
  /**\n
@{renderDoc("    *", property.doc, renderDocLink)}
    */\n
@end{}
@if{property.adder}
	@if{property.isMap}
  def @{property.adderMethod}(key: St@ring, value: @{toScalaTypeAndTypeParamTreatment(property.type, false)}):@{className}\n
	@else{}
  def @{property.adderMethod}(value: @{toScalaTypeAndTypeParamTreatment(property.type, false)}):@{className}\n
	@end{}
@end{}
@if{property.setter}
