  def @{property.setterMethod}(value: scala.collection.mutable.Buffer[@{toScalaTypeAndTypeParamTreatment(property.type, false)}]):@{className}\n
	@else{property.isSet}
  def @{property.setterMethod}(value: Set[@{toScalaTypeAndTypeParamTreatment(property.type, false)}]):@{className}\n
	@else{property.isMap}
  def @{property.setterMethod}(value: Map[String, @{toScalaTypeAndTypeParamTreatment(property.type, false)}]):@{className}\n
	@else{}
  def @{property.setterMethod}(value: @{toScalaTypeAndTypeParamTreatment(property.type, false)}):@{className}\n
	@end{}
@end{}
@if{(property.setter || property.adder) && property.getterMethod != null}
